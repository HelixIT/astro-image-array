---
export interface Props {
  images: object;
}

const { images } = Astro.props;
---

<p>Images Component</p>
{
  images.map((photo, index) => (
    <div>
      <p>{photo.url}</p>
      <Picture
        src={photo.url}
        widths={[200, 400, 800]}
        sizes="(max-width: 800px) 100vw, 800px"
        alt={photo.alt}
        aspectRatio="4:3"
      />
    </div>
  ))
}
